vb = \relative c'' {
  \voiceconsts

  \repeat volta 2 {
    \partial 4 h8(\mf a)
    g( a) h[ c] d4
    c8( a) c4 e8 d
    c4 c r
    g g r
    r8 a h[ c] a h
    g d g[ h] e( d)

    c4 c r
    g r
  }

  \repeat volta 2 {
    \partial 4 r
    d8\f r g r a r
    \acciaccatura g h d g[ d] e( d)
    d, d'( e)[ d]( fis) d
    \acciaccatura h g'4 g r

    g,8 fis g4 r
    r2 e'8( d)
    c4 c r
    g r
  }

  \repeat volta 2 {
    \partial 4 h8.\p h16
    h8 r ais r a r
    g r g r d\f g

    c, r d r fis r
    r g g r h8.\p h16
    h8 r ais r a r
    g r g r d\f g
    c,8 r d r fis g
    g4 r
  }

  \repeat volta 2 {
    \partial 4 d'8.\f d16

    d8 r e r fis r
    g4 g h16( g) d h
    c8 r a r d r
    h'4 g d8.\p d16

    d8 r e r fis r
    g4 g h16(\f g) d h
    c8 r a r d r
    h4 h
  }
}