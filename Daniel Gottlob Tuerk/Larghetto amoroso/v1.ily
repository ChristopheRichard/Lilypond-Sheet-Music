va = \relative c' {
  \voiceconsts
  \clef "treble"

  \repeat volta 2 {
    \partial 8 b8
    f'( es d) b'( a g)
    g4.( f4) b8
    b( a) b h( c) es,
    es4.( d4) d'8

    d( c) b b( a) a
    a( g) f f( e) g
    a( d) c c4 b8
    a f'16( e d c) c4 b8

    a( f') f f( es?) d
    c( cis) d d4 b8
    a( c) c c( b) e,
    f4 r8 r4
  }
  \repeat volta 2 {
    \partial 8 f8
    es'?4 d8 c( d) es

    f4 es8 d( es) f
    es4 d8 c4 d8
    f4 d8 c4 f,8
    b,8.( c16 b8) f'( b, h)

    h( c) c c4.
    r8 des( c) r des( c)
    b4.( a?4) es''8
    d( g) f f4 es8
    d b'16( a g f) f4 es8

    d( b') b b( as) f
    f( fis) g g4 es8
    d( f) f f( es) a,
    b4 r8 r4
  }
}