va = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    g4\mf h16( d)
    c8( h) a
    h( a) g
    fis e( d)

    g4\f h16( c)
    c8( h) a
    h g( fis)
    g4 r8
  }

  \repeat volta 2 {
    d4\p e16( fis)
    g8( a) h
    e,( fis) g
    fis a16( fis d8)

    g4\f h16( d)
    c8( h) a
    d, e( fis)
    g4 r8
  }
}
