vh = \relative c' {
    \voiceconsts
    \clef "bass"

    \repeat volta 2 {
    	    d4\p r a r
	    d r a r

	    d d g g
	    cis, cis fis fis

	    h, h e, e
	    a a a a

	    a2 a
	    a4 e a2
    }
    \repeat volta 2 {
    	    e fis4 r
	    h r h h

	    h h h r
	    h r d r

	    a r d r
	    d r g, r

	    g' r a, r
	    a a a a

	    d a d2
    }
}