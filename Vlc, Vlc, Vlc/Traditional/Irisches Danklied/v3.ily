vc = \relative c' {
   \voiceconsts

   a4\mf a8( a) gis4 e'
   fis8( fis) cis( a) cis4 cis
   d8 h d d a h cis a
   e'2. r4

   a, a8 a gis4 e'
   fis cis8( a) cis4 cis8( cis)
   d h d d e e cis h
   a2. r4

   \repeat volta 2 {
     d4.d8 h4 gis
     a a e' e
     a,8 a cis e a e cis a
     e'2. r4

     a, a gis e'
     fis cis8 a cis4 cis
     d8 h d d e e cis h
     a2. r4
   }
}