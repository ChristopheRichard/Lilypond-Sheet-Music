vi = \relative c {
  \voiceconsts
  \clef "bass"

  <d fis>2 <e g>
  <fis a> g~
  g fis

  fis1
  e2 a,
  d cis
  h a
  g1

  d'4 cis h a
  g1
  a
  <d fis>2 <e g>
  <fis a> <g h>

  << a1 \\ { g2 fis } >>
  fis1
  e2 a,
  << { a' fis4. d16 e } \\ { d2 cis } >>

  << { fis  fis4. d16 e } \\ { h2 a } >>
  << { fis'2. g4 } \\ g,1 >>
  << { a'2 h4 a } \\ { d, cis h a } >>
  << { g'2. a4 } \\ g,1 >>

  fis2 g
  a1 \bar "||" \time 3/4
  <ais fis'>2.
  <h fis'>2 <a! fis'>4 \bar "||" \time 2/4
  g2 \bar "||" \time 4/4 \rall
  a1
  <d, d'> \bar "|."
