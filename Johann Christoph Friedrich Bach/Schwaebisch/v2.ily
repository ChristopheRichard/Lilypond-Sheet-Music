vb = \relative c' {
  \voiceconsts
  \clef treble

  \repeat volta 2 {
    d8( c) h
    d4.
    d4 h16( cis)
    d4 s8

    d(\f c) h
    d4.~
    d8 d d16( c)
    h4 s8
  }

  \repeat volta 2 {
    fis'4 g16( a)
    g8( fis) <g d>
    e4 e8
    fis4 s8

    g4.
    fis8( g) a
    g <e g>( fis)
    g4 s8
 }
}
