  \clef "treble"

  \repeat volta 2 {
    \partial 4 fis4
    h( a) g( fis)
    e2 fis4\fermata cis'\upbow
    d d cis8( h) cis4 \boxa
    h2.\fermata d4
    cis8( h) a4 h cis

    d2.\fermata a4
    h( a g) g
    fis2.\fermata d'4 \boxb
    cis8( d e4) d( cis)
